 <animation>
  <type>rotate</type>
  <object-name>wheelLF</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>25</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>-25</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-1.49594</x-m>
   <y-m> -0.832716 </y-m>
   <z-m> 0.349806</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

<animation>
    <type>translate</type>
    <object-name>wheelLF</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <factor>.15</factor>
    <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
    </axis>
  </animation>

 <animation>
  <type>rotate</type>
  <object-name>wheelRF</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>25</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
