  <center>
   <x-m>-1.49594</x-m>
   <y-m>0.832716 </y-m>
    <z-m> 0.349806 </z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

<animation>
